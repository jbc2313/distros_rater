---

import Letter_S from '../../public/Letters/S.png'
import Letter_A from '../../public/Letters/A.png'
import Letter_B from '../../public/Letters/B.png'
import Letter_C from '../../public/Letters/C.png'
import Letter_F from '../../public/Letters/F.png'

---


<div id="TierMainDiv">
    <div id="S_Tier">
        <div id="letterbox"><img src={Letter_S} /></div>
        <div id="distrobox"></div>
    </div>
    <div id="A_Tier">
        <div id="letterbox"><img src={Letter_A} /></div>
        <div id="distrobox"></div>
    </div>
    <div id="B_Tier">
        <div id="letterbox"><img src={Letter_B} /></div>
        <div id="distrobox"></div>
    </div>
    <div id="C_Tier">
        <div id="letterbox"><img src={Letter_C} /></div>
        <div id="distrobox"></div>
    </div>
    <div id="F_Tier">
        <div id="letterbox"><img src={Letter_F} /></div>
        <div id="distrobox"></div>
    </div>
</div>

<style>
    #TierMainDiv {
        height: 63.5vh;
        width: 60vw;
        margin: auto;
        border: solid black 1px;
        display: flex;
        flex-direction: column;
        { /* justify-content: space-between; */ }
    }

    #letterbox img {
        height: 100%;
        object-fit: contain;
    }
    
    #S_Tier, 
    #A_Tier,
    #B_Tier,
    #C_Tier,
    #F_Tier { 
        display: flex;
        width: 60vw;
        height: 12.5vh;
        border: solid black 1px;

    }

    #letterbox {
        border: solid black 1px;
        border-right: solid black 3px;
        width: 10vw;
    }


</style>



